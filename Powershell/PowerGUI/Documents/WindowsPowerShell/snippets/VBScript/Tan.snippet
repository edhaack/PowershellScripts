<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>Tan</Title>
      <Shortcut>tan</Shortcut>
      <Description>Insert the PowerShell equivalent of the VBScript Tan function</Description>
      <Author>Quest Software, Inc.</Author>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
    </Header>
    <Snippet>
      <Declarations>
        <Literal>
          <ID>result</ID>
          <Default>$result</Default>
          <ToolTip>Variable to store the result value</ToolTip>
        </Literal>
        <Literal>
          <ID>angle</ID>
          <Default>$angle</Default>
          <ToolTip>Angle from which to calculate the tangent</ToolTip>
        </Literal>
      </Declarations>
      <Code Language="powershell">
        <![CDATA[$result$ = [Math]::Tan($angle$)$end$]]>
      </Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>
